{
  "name": "typescript-example",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:base": "tsc",
    "build:examples": "npm-run-all -p build:examples:*",
    "build:examples:app-node": "npm --prefix ./examples/app-node run build",
    "build:examples:app-node-next": "npm --prefix ./examples/app-node-next run build",
    "build:examples:lib": "npm --prefix ./examples/lib run build",
    "build:lib": "npm --prefix ./lib run build",
    "clean": "npm-run-all -p clean:*",
    "clean:examples": "npm-run-all -p clean:examples:*",
    "clean:examples:app-node": "npm --prefix ./examples/app-node run clean",
    "clean:examples:app-node-next": "npm --prefix ./examples/app-node-next run clean",
    "clean:examples:lib": "npm --prefix ./examples/lib run clean",
    "clean:lib": "npm --prefix ./lib run clean",
    "install": "npm-run-all -p install:*",
    "install:examples": "npm-run-all -p install:examples:*",
    "install:examples:app-node": "npm --prefix ./examples/app-node install",
    "install:examples:app-node-next": "npm --prefix ./examples/app-node-next install",
    "install:examples:lib": "npm --prefix ./examples/lib install",
    "install:lib": "npm --prefix ./lib install",
    "postinstall": "npm run build",
    "preinstall": "npm-run-all install:*",
    "test": "tsc"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "lib": "file:./lib"
  }
}
